<color-families>
    <page-header class="color-family-header" headline="Browse Paint Colors" subheadline="Beiges" backlinktext="Explore Different Colors" data-backurl="/Colors">
        <div id="PageHeader">
            <a id="HeaderBackLink">
                <span id="HeaderLeftArrow" class="material-icons">keyboard_arrow_left</span>
                <span id="HeaderBackLinkText" data-url="/color">Explore Different Colors</span>
            </a>
            <h1 id="HeaderPageTitle">Browse Paint Colors</h1>
            <span id="HeaderPageSubTitle">Beiges</span>
        </div>
    </page-header>
    <div id="MainContentLeftPane">
        <div id="ColorFamilyFamilies">
            <% for (idx in productList) {%>
            <div class="ColorItem" style="" data-index="<%= idx%>" data-id="<%= productList[idx].id%>" data-src="<%= productList[idx].src%>">
                <a id="ColorFamSelector" class="" style="background-image: url(<%= productList[idx].src%>); background-size: contain;">
                    <span class="material-icons isDark">check_circle</span>
                </a>
                <p class="SavedColorName">
                    <span class=""><%= productList[idx].title%></span>
                </p>
            </div>
            <%}%>
        </div>
    </div>
</color-families>