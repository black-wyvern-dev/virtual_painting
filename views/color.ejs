<colors class="ng-scope ng-isolate-scope">
    <page-header headline="Find Colors for Your Photo" backlinktext="Select a Different Photo" data-backurl="/" class="ng-isolate-scope">
        <div id="PageHeader">
            <a id="HeaderBackLink" ng-if="model.backlinktext" gtm-event="['ux', 'backButton', model.backlinktext]" class="ng-scope ng-isolate-scope" data-t="ux" data-d1="backButton" data-d2="Select a Different Photo">
                <span id="HeaderLeftArrow" class="material-icons">keyboard_arrow_left</span>
                <span id="HeaderBackLinkText" class="ng-binding" data-url="/photo">SÃ©lectionner une autre photo</span>
            </a>
            <h1 id="HeaderPageTitle" class="ng-binding">Trouver la couleur pour votre photo</h1>
        </div>
    </page-header>
    <div id="MainContentLeftPane">
        <div id="ColorsTileContainer">
            <a id="ColorsFamiliesTap" gtm-event="['ux', 'colorTile', 'color: ' + tab.TextKey + ' : ' + tab.DisplayOrder]" class="ng-scope ng-isolate-scope" data-t="ux" data-d1="colorTile" data-d2="color: Browse All Colors : 1">
                <tile data-lazysrc="https://visualizecolor.blob.core.windows.net/voc/colorspage/browseall.jpg" data-text="Browse All Colors" data-lazyalt="Browse All Colors" data-divclass="LazyImage" ng-click="model.Select(tab.Route)" class="ng-isolate-scope"><div id="TileCell" ng-class="model.cssClass()" class="HasText">
                    <div id="TileImageLoading" class="">
                        
                        <img id="TileImage" scroll-loaded-image="" data-lazysrc="https://visualizecolor.blob.core.windows.net/voc/colorspage/browseall.jpg" data-lazyalt="" data-parentclass="TileImageLoader" src="https://visualizecolor.blob.core.windows.net/voc/colorspage/browseall.jpg" alt="">
                    </div>
                    <h4 ng-if="model.text" name="TileText" class="ng-binding ng-scope">Rechercher toutes les couleurs</h4>
                </div></tile>
            </a>
            <a id="ColorsCollectionTap" gtm-event="['ux', 'colorTile', 'color: ' + tab.TextKey + ' : ' + tab.DisplayOrder]" class="ng-scope ng-isolate-scope" data-t="ux" data-d1="colorTile" data-d2="color: View Curated Palettes : 2">
                <tile data-lazysrc="https://visualizecolor.blob.core.windows.net/voc/colorspage/collections.jpg" data-text="View Curated Palettes" data-lazyalt="View Curated Palettes" data-divclass="LazyImage" ng-click="model.Select(tab.Route)" class="ng-isolate-scope"><div id="TileCell" ng-class="model.cssClass()" class="HasText">
                    <div id="TileImageLoading" class="">
                        
                        <img id="TileImage" scroll-loaded-image="" data-lazysrc="https://visualizecolor.blob.core.windows.net/voc/colorspage/collections.jpg" data-lazyalt="" data-parentclass="TileImageLoader" src="https://visualizecolor.blob.core.windows.net/voc/colorspage/collections.jpg" alt="">
                    </div>
                    <h4 ng-if="model.text" name="TileText" class="ng-binding ng-scope">Voir nos palettes de couleurs</h4>
                </div></tile>
            </a>
        </div>
    </div>
</colors>